{
  "name": "my-react-library",
  "version": "0.3.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "main": "./dist/lib.umd.js",
  "module": "./dist/lib.module.js",
  "source": "src/components/Develop/Develop.tsx",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "yarn build:lib && yarn build:umd",
    "build:umd": "cross-env BUILD_PATH=build/umd BUILD_EXTERNALS=false react-app-rewired build",
    "build:lib": "cross-env BUILD_PATH=build/lib BUILD_EXTERNALS=false  react-app-rewired build",
    "eject": "react-scripts eject",
    "dev": "concurrently \"react-scripts start\" \"php -S localhost:3001 -t api\"",
    "docs": "typedoc",
    "build:bundle": "microbundle",
    "b": "microbundle --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "globals": {
      "EndUserConstants": "readonly",
      "EndUser": "readonly"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "dompurify": "^3.1.6",
    "mime": "^4.0.4",
    "styled-components": "^6.1.12"
  },
  "devDependencies": {
    "euscp": "file:packages/euscp-1.3.72.tgz",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^16.18.104",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "microbundle": "^0.15.1",
    "nodemon": "^3.1.4",
    "react": "^18.0.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "typedoc": "^0.26.5",
    "typedoc-material-theme": "^1.1.0",
    "typedoc-theme-hierarchy": "^5.0.3",
    "typescript": "^4.9.5"
  }
}
